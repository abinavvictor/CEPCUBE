<script type = "text/javascript">
    $(document).ready(function() {
        $.getJSON('http://localhost:3000/tableData', function (data) {

            var record = '';

            $.each(data, function (key, value){
                record += '<tr>';
                record += '<td>'+value.school+'</td>';
                record += '<td>'+value.cohort +'</td>';
                record += '<td>'+value.cep_program +'</td>';
                record += '<td>'+value.cep_id +'</td>';
                record += '<td>'+value.firstname +'</td>';
                record += '<td>'+value.lname +'</td>';
                record += '<td>'+value.gender +'</td>';
                record += '<td>'+value.ethnicity+'</td>';
                record += '</tr>'
            });
            $('#import_table').append(record);
        });

    });


</script>
